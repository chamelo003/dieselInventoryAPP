<!DOCTYPE html>
<html lang="es-ES">
<% include ../../../Layouts/Ordenes/Header.ejs %>
<h3>Editar datos del Transportista</h3>
<div class="row d-flex justify-content-center">
	<form class="text-center border border-light p-5 col-md-4" action="/catalogos/trans/edit/<%= results[0].RTN %>" method="POST">
			<!-- Grid row -->
			<div class="form-row">
			<!-- Grid column -->
			<div class="col-lg-12">
					<!-- Material input -->
					<div class="md-form mt-0">
							<input id="rtnTrans" name="rtn" type="text" class="form-control" maxlength="14" autocomplete="off" value="<%= results[0].RTN %>" readonly="readonly" >
							<label for="rtnTrans">RTN</label>
					</div>
			</div>
			<!-- Grid column -->
			<div class="md-form col-lg-12">
					<!-- Material input -->
					<div class="md-form mt-0">
							<input id="nombre" name="nom" type="text" class="form-control" autocomplete="off" required value="<%= results[0].Nombre %>">
							<label for="nombre">Nombres</label>
					</div>
			</div>
			<!-- Grid column -->
			<div class="md-form col-lg-12">
					<!-- Material input -->
					<div class="md-form mt-0">
							<input id="apellido" name="ape" type="text" class="form-control" autocomplete="off" required value="<%= results[0].Apellido %>">
							<label for="apellido">Apellidos</label>
					</div>
			</div>                          
			<!-- Grid column -->
			<div class="md-form col-lg-12">
					<textarea id="form7" name="obs" class="md-textarea form-control" rows="3" autocomplete="off"><%= results[0].Observacion %></textarea>
					<label for="form7">Observacion</label>
			</div>
			<div class="col-md-12">
					<button class="btn btn-primary" type="submit">Guardar Cambios</button>
			</div>
			<!-- Grid row -->
	</form> 
</div>    
<% include ../../../Layouts/Ordenes/Footer.ejs %>
</html>